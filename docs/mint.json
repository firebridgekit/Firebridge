{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Firebridge",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
